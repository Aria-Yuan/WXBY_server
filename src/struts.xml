<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="default" namespace="/"
             extends="struts-default,json-default">
        <action name="login" class="com.imudges.web.action.LoginAction">
            <result name="success" type="json">
                <param name="root">result</param>
            </result>
        </action>

        <action name="searchFirm" class="com.imudges.web.action.FirmAction">
            <result name="success" type="json">
                <param name="root">result</param>
            </result>
        </action>

        <action name="searchCounseling" class="com.imudges.web.action.CounselingAction">
            <result name="success" type="json">
                <param name="root">result</param>
            </result>
        </action>

        <action name="updateCounseling" class="com.imudges.web.action.CounselingUpdateAction">
            <result name="success" type="json">
                <param name="root">result</param>
            </result>
        </action>

        <action name="searchLaw" class="com.imudges.web.action.LawAction">
            <result name="success" type="json">
                <param name="root">result</param>
            </result>
        </action>
    </package>
</struts>